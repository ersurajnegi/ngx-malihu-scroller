{
  "name": "ngx-malihu-scroller",
  "version": "1.0.1",
  "description": "Angular directive for custom scroller for angular web apps",
  "main": "index.umd.js",
  "jsnext:main": "index.esm.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js",
    "tsc": "tsc",
    "after": "node tools/removecode.js && node tools/copy-package.js && node tools/cleanup.js",
    "copy": "node tools/copy-package.js && node tools/cleanup.js",
    "publish": "cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersurajnegi/ngx-malihu-scroller.git"
  },
  "author": "Suraj Negi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ersurajnegi/ngx-malihu-scroller/issues"
  },
  "homepage": "https://github.com/ersurajnegi/ngx-malihu-scroller#readme",
  "dependencies": {
    "@angular/core": "^4.2.6"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "node-sass": "^4.5.3",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1"
  }
}
