  "dependencies": {
    "@angular/core": "^4.3.1"
  },
  "devDependencies": {
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "del": "^3.0.0",
    "rollup": "^0.45.2",
    "rxjs": "^5.4.2",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.25",
    "zone.js": "^0.8.14"
